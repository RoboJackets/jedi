{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "bugsnag/bugsnag-laravel": "^2.20",
        "doctrine/dbal": "*",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.7",
        "guzzlehttp/guzzle": "^6.3",
        "kelvinmo/simplejwt": "^0.2.5",
        "laravel/framework": "^6.18",
        "laravel/horizon": "^3.7",
        "laravel/slack-notification-channel": "^2.2",
        "laravel/tinker": "^2.0",
        "predis/predis": "^1.1",
        "robojackets/error-pages": "^2.0",
        "robojackets/network-check": "^1.0",
        "sensiolabs/security-checker": "^6.0",
        "spatie/laravel-failed-job-monitor": "^3.3",
        "spatie/laravel-webhook-client": "*",
        "subfission/cas": "*",
        "ukfast/laravel-health-check": "^1.7"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.6",
        "phpstan/extension-installer": "*",
        "robojackets/static-analysis": "*",
        "facade/ignition": "^1.4",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "classmap-authoritative": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.2"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.robojackets.org"
        }
    ]
}
